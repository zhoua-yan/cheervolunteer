<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 
	<welcome-file-list>
		<welcome-file>/login.html</welcome-file>
	</welcome-file-list>
 	
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>cheer.login.handleRegister</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/helpRegister</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>cheer.login.handleLogin</servlet-class>
		
	</servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>handleRegister</servlet-name>
    <servlet-class>cheer.login.handleRegister</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>HandleDatabase</servlet-name>
    <servlet-class>cheer.login.HandleDatabase</servlet-class>
  </servlet>


	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/helpLogin</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>handleRegister</servlet-name>
    <url-pattern>/servlet/handleRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HandleDatabase</servlet-name>
    <url-pattern>/servlet/HandleDatabase</url-pattern>
  </servlet-mapping>
	
	<servlet>
		<servlet-name>lookRecord</servlet-name>
		<servlet-class>cheer.login.HandleDatabase</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>lookRecord</servlet-name>
		<url-pattern>/helpShowMember</url-pattern>
	</servlet-mapping>
	
</web-app>
